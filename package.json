  "scripts": {
    "lint": "eslint src",
    "test": "NODE_ENV=test ava",
    "test:cov": "NODE_ENV=test nyc ava",
    "watch": "webpack-dev-server --hot",
    "build:dev": "NODE_ENV=development webpack",
    "build": "NODE_ENV=production webpack",
    "build:size": "NODE_ENV=production webpack --json | webpack-bundle-size-analyzer",
    "build:ci": "yarn lint && yarn test:cov && yarn build",
    "deploy": "yarn build && netlify deploy",
    "storybook": "start-storybook -p 6006"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/setup"
    ],
    "files": [
      "src/**/*.spec.js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.38",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.38",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.38",
    "@babel/preset-env": "7.0.0-beta.38",
    "@babel/preset-flow": "7.0.0-beta.38",
    "@babel/preset-react": "7.0.0-beta.38",
    "@babel/register": "7.0.0-beta.38",
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addon-storyshots": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "ava": "^0.24.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "compression-webpack-plugin": "^1.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.6.0",
    "flow-bin": "^0.64.0",
    "html-webpack-plugin": "^2.30.1",
    "nyc": "^11.2.1",
