    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.0.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "workbox-webpack-plugin": "^2.1.2"
  }
